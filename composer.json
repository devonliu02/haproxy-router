{
    "name" : "rethink/haproxy-router",
    "description": "A service router and api gateway based on HAProxy",
    "authors" : [
        {
            "name" : "Jin Hu",
            "email": "bixuehujin@gmail.com"
        }
    ],
    "require": {
        "blink/blink": "dev-master",
        "acmephp/core": "^1.0@beta",
        "blink/laravel-database": "dev-master",
        "illuminate/validation": "5.3.x-dev",
        "blink/i18n": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "files": [
            "src/support/helpers.php"
        ],
        "psr-4": {
            "rethink\\hrouter\\": "src",
            "rethink\\hrouter\\tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "vendor-dir": "vendor"
    }
}
